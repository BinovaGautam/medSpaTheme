  "agentOperations": {
    "assignAgent": {
      "operation": "assign-agent-to-workflow",
      "requiredFields": ["agentId", "workflowId", "taskType"],
      "validation": ["agent-available", "capacity-available", "capability-match"]
    },
    "releaseAgent": {
      "operation": "release-agent-from-workflow",
      "requiredFields": ["agentId", "workflowId"],
      "validation": ["agent-assigned", "workflow-exists"]
    },
    "updateAgentStatus": {
      "operation": "update-agent-availability-status",
      "requiredFields": ["agentId", "newStatus"],
      "validation": ["agent-exists", "valid-status"]
    },
    "healthCheck": {
      "operation": "verify-agent-operational-status",
      "requiredFields": ["agentId"],
      "validation": ["agent-exists"]
    }
